{
  "name": "simple-assistant",
  "version": "1.5.0",
  "description": "Chrome extension for SimpleMMO that automates certain aspects of the game",
  "main": "src/assets/content.js",
  "scripts": {
    "clean": "IF EXIST dist rmdir /Q /S dist",
    "buildDev": "webpack --mode development --config webpack.dev.config.js",
    "buildProd": "npm run clean && webpack --mode production --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SugarF0x/simple-assistant.git"
  },
  "author": "Sugar_F0x",
