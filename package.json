  "bugs": {
    "url": "https://github.com/SugarF0x/simple-assistant/issues"
  },
  "homepage": "https://github.com/SugarF0x/simple-assistant#readme",
  "dependencies": {
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.1",
    "copy-webpack-plugin": "^6.1.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
